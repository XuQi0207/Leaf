$(TOPINCLUDE):
	-mkdir -p $@
	chown root $@
	chgrp bin $@
	chmod 0755 $@

$(DESTDIR):
	-mkdir -p $@
	chown root $@
	chgrp bin $@
	chmod 0755 $@

install.man: $(FILMANDIR) $(LIBMANDIR) $(MANPAGES)

$(FILMANDIR):
	-mkdir -p $@
	chown root $@
	chgrp bin $@
	chmod 0755 $@

$(LIBMANDIR):
	-mkdir -p $@
	chown root $@
	chgrp bin $@
	chmod 0755 $@

$(FILMANDIR)/Castor_limits.$(FILMANSUFFIX):	Castor_limits.man
	nroff -man Castor_limits.man > $(FILMANDIR)/Castor_limits.$(FILMANSUFFIX)
$(LIBMANDIR)/serrno.$(LIBMANSUFFIX):	serrno.man
	nroff -man serrno.man > $(LIBMANDIR)/serrno.$(LIBMANSUFFIX)
$(LIBMANDIR)/sacct.$(LIBMANSUFFIX):	sacct.man
	nroff -man sacct.man > $(LIBMANDIR)/sacct.$(LIBMANSUFFIX)
$(EXPORTMAN)/man4/Castor_limits.$(FILMANSUFFIX):	Castor_limits.man
	cp Castor_limits.man $(EXPORTMAN)/man4/Castor_limits.$(FILMANSUFFIX)
$(EXPORTMAN)/man3/serrno.$(LIBMANSUFFIX):	serrno.man
	cp serrno.man $(EXPORTMAN)/man3/serrno.$(LIBMANSUFFIX)
$(EXPORTMAN)/man3/sacct.$(LIBMANSUFFIX):	sacct.man
	cp sacct.man $(EXPORTMAN)/man3/sacct.$(LIBMANSUFFIX)

clean:
	@echo cleaning in `pwd`
	-@$(RM) -f *.o *.a ,* .es1 core

clobber:        clean
	@echo clobbering in `pwd`

Makefiles:

FORCE:

